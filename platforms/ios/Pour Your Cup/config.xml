<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pouryourcup.pouryourcup" ios-CFBundleVersion="27" version="1.2.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Badge">
        <param name="ios-package" value="APPBadge" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="Contacts">
        <param name="ios-package" value="CDVContacts" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="PushNotification">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <name>Pour Your Cup</name>
    <description>
        The MicroGenerosity app for the Generosity Economy
    </description>
    <author email="hello@pouryourcup.com" href="https://www.pouryourcup.com">
        Raymond Tsao - Pour Your Cup
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="60" src="/www/img/ios/app-icons/Icon-60.png" width="60" />
    <icon height="120" src="/www/img/ios/app-icons/Icon-60@2x.png" width="120" />
    <icon height="180" src="/www/img/ios/app-icons/Icon-60@3x.png" width="180" />
    <icon height="76" src="/www/img/ios/app-icons/Icon-76.png" width="76" />
    <icon height="152" src="/www/img/ios/app-icons/Icon-76@2x.png" width="152" />
    <icon height="40" src="/www/img/ios/app-icons/Icon-40.png" width="40" />
    <icon height="80" src="/www/img/ios/app-icons/Icon-40@2x.png" width="80" />
    <icon height="57" src="/www/img/ios/app-icons/Icon.png" width="57" />
    <icon height="114" src="/www/img/ios/app-icons/Icon@2x.png" width="114" />
    <icon height="72" src="/www/img/ios/app-icons/Icon-72.png" width="72" />
    <icon height="144" src="/www/img/ios/app-icons/Icon-72@2x.png" width="144" />
    <icon height="29" src="/www/img/ios/app-icons/Icon-Small.png" width="29" />
    <icon height="58" src="/www/img/ios/app-icons/Icon-Small@2x.png" width="58" />
    <icon height="87" src="/www/img/ios/app-icons/Icon-Small@3x.png" width="87" />
    <icon height="50" src="/www/img/ios/app-icons/Icon-50.png" width="50" />
    <icon height="100" src="/www/img/ios/app-icons/Icon-50@2x.png" width="100" />
    <splash height="480" src="/www/img/ios/launch/320x480.png" width="320" />
    <splash height="960" src="/www/img/ios/launch/640x960.png" width="640" />
    <splash height="1024" src="/www/img/ios/launch/768x1024.png" width="768" />
    <splash height="2048" src="/www/img/ios/launch/1536x2048.png" width="1536" />
    <splash height="1136" src="/www/img/ios/launch/640x1136.png" width="640" />
    <splash height="1334" src="/www/img/ios/launch/750x1334.png" width="750" />
    <splash height="2208" src="/www/img/ios/launch/1242x2208.png" width="1242" />
    <access allows-arbitrary-loads-in-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
